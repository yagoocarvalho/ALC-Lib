<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <!--FUNCTION PARAMETERS-->
      <add key="tolerance" value="5*10^-4"/>
      
      <!--BISSECTION-->
      <add key="a"         value="0"/>
      <add key="b"         value="10"/>
      <!--NEWTON ORIGINAL-->
      <add key="x0"        value="10"/>
      <add key="nIter"     value="20"/>
      <!--NEWTON SECANT-->
      <add key="dx"        value="1*10^-3"/>
      
      <!--SOLVING METHOD-->
      <add key="method"    value="newton-secant"/>
    </appSettings>
</configuration>