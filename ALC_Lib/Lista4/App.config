<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <!--FUNCTION PARAMETERS-->
      <add key="tolerance" value="5*10^-4"/>
      
      <!--BISSECTION-->
      <add key="a-bissec"  value="0"/>
      <add key="b-bissec"  value="10"/>
      <!--NEWTON ORIGINAL-->
      <add key="x0"        value="10"/>
      <add key="nIter"     value="20"/>
      <!--NEWTON SECANT-->
      <add key="dx"        value="1*10^-3"/>
      <!--INVERSE INTERPOLATION-->
      <add key="a-interp"         value="3"/>
      <add key="b-interp"         value="5"/>
      <add key="c-interp"         value="10"/>

      <!--SOLVING METHOD-->
      <add key="method"    value="all"/>
    </appSettings>
</configuration>